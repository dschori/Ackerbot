<launch>

  <!-- Launch Velodyne Node -->
  <include file="$(find velodyne_pointcloud)/launch/VLP16_points.launch" />

<!--node pkg="tf" type="static_transform_publisher" name="laser_to_velodyne2" args="0 0 0 0 0 0 base_link velodyne 100"></node-->

<!-- Launch Realsense T265 Node -->
  <include file="$(find realsense2_camera)/launch/rs_t265.launch" />
<!--node pkg="tf" type="static_transform_publisher" name="t265_to_base" args="0 0 0 0 0 0 camera_t265_pose_frame base_link 100"></node-->

<node pkg="topic_tools" type="throttle" name="image_throttle" args="messages /camera_t265/fisheye1/image_raw 2.0 /camera_t265/image_throttled"/>

<node pkg="tf" type="static_transform_publisher" name="base_to_t265" args="0 0 0 0 0 0 camera_t265_pose_frame base_link 10"></node>

<node pkg="tf" type="static_transform_publisher" name="base_to_velodyne" args="0 -0.08 0.06 0 0 0 base_link velodyne 10"></node>
</launch>
